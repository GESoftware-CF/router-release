<%=
  def server_private_key
    if_p('tcp_router.server_private_key') do |prop|
      return prop
    end.else do
      if_link('tcp_router') do |link|
        return link.p('tcp_router.mtls_server_key')
      end
    end
    raise RuntimeError, 'Routing API server private key not found in properties nor in tcp_router Link. This value can be specified using the tcp_router.server_private_key property.'
  end

  server_private_key
-%>
